fn str_to_int_list(s) {
    let slist = strtok(s, " \t");
    apply(atoi, slist)
}

fn main() {
    fopen_read @ fin = "example/magic_number.txt";
    readstr @ fin >> s;
    let slist = strtok(s, " \t");

    let xlist = str_to_int_list(s);

    let y = *(
        retrieve_nth(xlist, 1),
        retrieve_nth(xlist, 2)
    );
    writeline @ stdout << y;

    0
}